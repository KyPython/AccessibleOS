{
  "name": "accessibleos",
  "version": "1.0.0",
  "description": "AccessibleOS - Accessible Task Management App (Web, Mobile, Unity)",
  "private": true,
  "workspaces": [
    "backend",
    "web",
    "mobile"
  ],
  "scripts": {
    "install:all": "npm install && npm run install:backend && npm run install:web && npm run install:mobile",
    "install:backend": "cd backend && npm install",
    "install:web": "cd web && npm install",
    "install:mobile": "cd mobile && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:web": "cd web && npm run dev",
    "test:backend": "cd backend && npm test",
    "test:web": "cd web && npm test",
    "test:all": "npm run test:backend && npm run test:web",
    "build:backend": "cd backend && npm run build",
    "build:web": "cd web && npm run build"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "accessibility",
    "a11y",
    "task-management",
    "react",
    "react-native",
    "unity"
  ]
}

