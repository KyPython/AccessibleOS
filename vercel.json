{
  "buildCommand": "cd web && npm install && npm run build",
  "outputDirectory": "web/dist",
  "devCommand": "cd web && npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@api_url"
  }
}

